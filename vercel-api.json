{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.py"
    }
  ],
  "functions": {
    "backend/api/index.py": {
      "maxDuration": 10
    }
  },
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
